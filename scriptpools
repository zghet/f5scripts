
#!/bin/sh

#Este script esta dise침ado para que a partir de un nombre de pools podamos sacar una relaci칩n de instancias en cada balanceo

POOL=`cat pool_inventory.txt` #En este archivo tenemos una relaci칩n de los pools que deseamos sacar una relaci칩n

for ipremota in `cat inventory_file.txt`
do

        ssh root@$ipremota tmsh list ltm pool $POOL | grep  "ltm pool\| 10.*"  >> instancias.txt

done
